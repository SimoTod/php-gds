{
    "name": "tomwalder/php-gds",
    "type": "library",
    "description": "Google Cloud Datastore Library for PHP",
    "keywords": ["google", "datastore", "appengine", "gae", "cloud datastore", "nosql"],
    "homepage": "https://github.com/tomwalder/php-gds",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Tom Walder",
            "email": "twalder@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-bcmath" : "*"
    },
    "require-dev": {
        "google/auth": "^1.0",
        "google/appengine-php-sdk": "^1.9.30",
        "google/cloud-datastore": "v1.10.0",
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "v2.0.*"
    },
    "suggest": {
        "google/auth": "If you need to use the REST API or Firestore in Datastore mode.",
        "google/cloud-datastore": "If you need to use the gRPC API (for speed) or Firestore in Datastore mode."
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {"GDS\\": "src/GDS/"}
    },
    "include-path": ["src/"]
}
